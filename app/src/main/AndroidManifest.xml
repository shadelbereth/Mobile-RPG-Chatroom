<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="be.interface3.sroch.gameproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RoomListActivity" />
        <activity android:name=".CreateRoomActivity" />
        <activity android:name=".DisplayRoomActivity" />
        <activity android:name=".AddCharacterActivity" />
        <activity android:name=".CreateCharacterActivity" />
        <activity android:name=".ConfirmActivity" />
        <activity android:name=".RoomAdministrationActivity"></activity>
    </application>

</manifest>